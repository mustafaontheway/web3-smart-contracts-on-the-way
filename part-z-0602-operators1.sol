pragma solidity ^0.8.24;

contract Operator {

// Declare two variables of type uint256 with the names 'a' and 'b'. Initialize 'a' with 10 and 'b' with 2. Then, use the multiplication assignment operator to multiply 'a' by 'b'

    uint256 a = 10;

    uint256 b = 2;

    a *= b;
}
